{
  "name": "pos-market-system",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev:frontend": "npm run dev -w frontend",
    "dev:backend": "npm run start:dev -w backend",
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "install:all": "npm install -w frontend && npm install -w backend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
